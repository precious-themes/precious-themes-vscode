  "tokenColors": [
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": [
        "constant.other.option",
        "constant.other.placeholder",
        "entity.name.variable",
        "keyword.operator",
        "keyword.operator.negation.regexp",
        "meta.definition.variable.name",
        "meta.embedded",
        "meta.function-call.arguments",
        "meta.function.parameters",
        "meta.indexed-name",
        "meta.template.expression",
        "punctuation",
        "source.groovy.embedded",
        "storage.modifier.chomping-indicator.yaml",
        "storage.modifier.flag.regexp",
        "storage.modifier.import.java",
        "storage.modifier.package.java",
        "string meta.image.inline.markdown",
        "string.unquoted",
        "support.other.parenthesis.regexp",
        "support.variable",
        "text",
        "variable",
        "variable.language.wildcard.java"
      ],
      "settings": {
        "foreground": "#4e5359"
      }
    },
    {
      "scope": [
        "entity.name.label",
        "keyword.operator.heredoc.php"
      ],
      "settings": {
        "foreground": "#4e5359",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "variable.parameter.preprocessor"
      ],
      "settings": {
        "foreground": "#4e5359",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "comment string.quoted"
      ],
      "settings": {
        "foreground": "#575c61",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.comment.begin",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#7f8080",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword.other.documentation",
        "keyword.other.phpdoc.php",
        "comment entity.other.attribute-name",
        "storage.type.class.doxygen"
      ],
      "settings": {
        "foreground": "#7f8080",
        "fontStyle": "italic underline"
      }
    },
    {
      "scope": [
        "comment entity.name.tag",
        "meta.shebang.shell"
      ],
      "settings": {
        "foreground": "#7f8080",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": [
        "invalid",
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#b14745"
